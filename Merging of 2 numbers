#include<stdio.h>
#include<conio.h>
int main()
{
int arr[5]={1,2,6,9,0};
int arr2[5]={3,4,6,7,8};
int arr3[10];
int i,j;
for(i=0;i<5;i++)
{
arr3[i]=arr[i];
}
for(j=0;j<5;j++)
{
arr3[i]=arr2[j];
i++;
}
printf("\n the arranged array:");
for(i=0;i<10;i++)
{
printf("%d\n",arr3[i]);
}
getch();
return 0;
}
